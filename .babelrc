{
  "plugins": [
    [
      "import",
      { "libraryName": "antd", "libraryDirectory": "es", "style": "css" } // `style: true` for less
    ],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "hooks/*": ["src/hooks/*"],
          "api/*": ["src/api/*"],
          "mockData/*": ["src/mockData/*"],
          "pages/*": ["src/app/pages/*"],
          "shared/*": ["src/app/shared/*"],
          "static/*": ["src/static/*"],
          "types/*": ["src/types/*"],
          "utils/*": ["src/types/*"]
        }
      }
    ]
  ]
}
